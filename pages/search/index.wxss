.tit_seabox {
  width: calc(100% - 64rpx);
  background: #fff;
  height: 60rpx;
  padding: 20rpx 32rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}

.tit_seabox_bar {
  width: calc(100% - 32rpx);
  height: 60rpx;
  display: flex;
  align-items: center;
  border-radius: 30rpx;
  background: #F5F5F5;
  padding-left: 32rpx;
}

/*有权限添加活动*/
.tit_seabox_bar.tit_seabox_add {
  width: calc(100% - 122rpx);
}

/*开始搜索时*/
.tit_seabox_bar.tit_start_search {
  width: calc(100% - 102rpx);
}

/*开始搜索且有权限添加*/
.tit_seabox_bar.tit_start_search.tit_seabox_add {
  width: calc(100% - 192rpx);
}

.tit_seabox_bar icon {
  margin-right: 20rpx;

}

.tit_seabox input {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  width: 100%;
  margin-right: 32rpx;

}

.activity_add {
  width: 60rpx;
  text-align: right;
  border-left: 4rpx solid #f2f2f2;
  margin-left: 20rpx;

}

.activity_add image {
  width: 40rpx;
  height: 40rpx;
}

.activity_seabtn {
  font-size: 28rpx;
  width: 70rpx;
  text-align: right;
}

/* 搜索列表 */
.searchList {
  padding: 40rpx 0;
  border-bottom: solid rgb(240, 240, 240) 1rpx;
  background-color: #fff;

}

.searchType {
  color: #999;
  font-size: 30rpx;
  border: solid rgb(240, 240, 240) 1rpx;
  width: 100%;
  padding: 0 40rpx;
  line-height: 86rpx;
}

.searchList image {
  width: 128rpx;
  height: 188rpx;
  background-color: pink;
  margin-left: 40rpx;
}

.searchList .list {
  display: flex;
}
page {
  background-color: rgb(245, 245, 245);
}
.searchList .title {
  font-size: 34rpx;
  color: #222;
  font-weight: 700;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 360rpx;
}

.searchList .info {
  font-size: 26rpx;
  color: #666;
}

.searchList .listInfo {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 30rpx;
}

.searchList .rightBtn {
  position: absolute;
  right: 30rpx;
}

.searchList .btn {
  position: absolute;
  right: 0rpx;
  width: 106rpx;
  height: 54rpx;
  text-align: center;
  line-height: 56rpx;
  font-size: 24rpx;
  margin-top: 50rpx;
}

.rightBtn .btnLike {
  background-color: rgb(250, 175, 0)
}

.rightBtn .btnBy {
  background-color: rgb(239, 66, 56);
}

.likeWatch {
  color: #ffaa00;
  font-size: 32rpx;
}

.like {
  font-size: 24rpx;
}
.noMore {
  font-size: 26rpx;
  text-align: center;
  color: #999;
  line-height: 100rpx;
}